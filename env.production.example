# ============================================
# SecureVault Production Environment
# ============================================
# Copy to .env and fill in real values
# NEVER commit secrets to version control
# ============================================

# Database (use strong password in production!)
POSTGRES_USER=securevault
POSTGRES_PASSWORD=CHANGE_ME_use_strong_password
POSTGRES_DB=securevault

# Security Keys - MUST be unique per deployment
# Generate with: openssl rand -hex 32
JWT_SECRET_KEY=CHANGE_ME_generate_jwt_secret_key
ENCRYPTION_MASTER_KEY=CHANGE_ME_generate_encryption_master_key

# CORS - Set to your actual domain
ALLOWED_ORIGINS=https://yourdomain.com

# Application
ENVIRONMENT=production
DEBUG=false
