# ============================================
# SecureVault Environment Configuration
# ============================================
# Copy this file to .env and fill in the values
# NEVER commit the .env file to version control
# ============================================

# Application
ENVIRONMENT=development
DEBUG=true

# Database
POSTGRES_USER=securevault
POSTGRES_PASSWORD=securevault_dev_password
POSTGRES_DB=securevault

# Security - JWT
# Generate with: openssl rand -hex 32
JWT_SECRET_KEY=CHANGE_ME_generate_with_openssl_rand_hex_32

# Security - Encryption
# Generate with: openssl rand -hex 32
ENCRYPTION_MASTER_KEY=CHANGE_ME_generate_with_openssl_rand_hex_32

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
